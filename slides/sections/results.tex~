\section{Results}

\begin{frame}
	\frametitle{Data}
	\begin{itemize}
		\item 39 C projects
		\item 12 Java projects
		\item 56233 files converted
		\item 606118 methods analyzed
		\item 23 (0,04\%) files excluded from analysis
	\end{itemize}
\end{frame}

